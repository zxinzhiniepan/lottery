        <style>
            header {
                position: relative;
                height: 50px;
                background: #c84948;
            }

            .headtop {
                margin: 0;
                padding: 0;
            }

            .headtop > a {
                position: absolute;
                display: inline-block;
            }

            .headtop > a:first-child {
                top: 10px;
                left: 15px;
                width: 28px;
                height: 28px;
                font-size: 30px;
                font-weight: bold;
                color: #fff;
            }

            .headtop > a:last-child {
                top: 10px;
                right: 35px;
                display: inline-block;
                background-size: cover;;
                font-size: 16px;
                color: #FFF5EE;
                padding: 5px;
            }

            .headtop > h4 {
                text-align: center;
                padding: 14px 0;
                font-size: 21px;
                color: #fff;
            }

            .lottery {
                display: flex;
                padding: 10px 15px;
                background: #fff;
            }

            .lottery > p {
                width: 80%;
            }

            .lottery > p:first-child {
                color: #949494;
            }

            .lottery > p:first-child font {
                color: #ee3030;
            }

            .lottery > div {
                float: right;
                width: 100px;
                text-align: center;
                font-size: 10px;
                color: #949494;
            }

            .lottery > div > h6 {
                font-size: 18px;
                color: #ee3030;
            }

            /*列表*/
            .list {
                background-color: white;
                margin-top: 25px;
            }

            .row {
                padding: 0px 10px;
            }

            .col {
                /*border: 1px dashed black;*/
                border-bottom: 1px solid #e0e0e0;
                height: 40px;
                padding: 0px;
            }

            div.col:nth-of-type(3) {
                border-right: 1px solid #e0e0e0;
            }

            /*第一行标题*/
            .title {
                font-size: 16px;
                position: absolute;
                margin-top: 8px;
                margin-left: 12px;
            }

            /*圆*/
            .num-wrap {
                /*border: 1px solid #c84948;*/
                display: inline-block;
                height: 30px;
                width: 30px;
                position: absolute;
                top: 5px;
                left: 12px;
                -webkit-border-radius: 20px;
                -moz-border-radius: 20px;
                border-radius: 20px;
            }

            /*调整号码位置*/
            .num {
                position: absolute;
                top: 5px;
                left: 6px;
                color: white;
            }

            /*调整号码颜色*/
            .blue {
                background-color: #4a80ff;
            }

            .red {
                background-color: #ff1932;
            }

            .green {
                background-color: #20cb09;
            }

            /*调整赔率位置*/
            .chance {
                display: inline-block;
                margin-top: 10px;
                margin-left: 20px;
            }

            /*调整金额输入框位置*/
            .money {
                width: 70%;
                margin-top: 8px;
                margin-left: 6px;
                outline: none;
                border: 1px solid gray;
                text-indent: 3px;
                border-radius: 3px;
            }

            .content li {
                overflow: hidden;
                zoom: 1;
                padding: 8px;
                border-bottom: 1px #ddd dotted;
            }

            em {
                font-style: normal;
                font-weight: normal
            }

            .submit-wrap {
                text-align: center;
                margin-bottom: 20px;
            }
            /*提交按钮*/
            .submit {
                width: 80%;
                margin-top: 20px;
            }

            /*弹出框组件距离顶部的位置*/
            #my-modal {
                margin-top: 50px;
            }

            /*提示框*/
            .alert-container {
                text-align: center;
            }

            .alert-title {
                padding: 5px 0px;
            }

            .alert-title:nth-of-type(2) {
                border-right: 1px solid #e3e3e3;
            }

            .alert-info-wrap {
                text-align: center;
            }
            /*已购买提示信息*/
            .alert {
                background-color: #e3e3e3;
                padding: 20px 50px;
                -webkit-border-radius: 5px;
                -moz-border-radius: 5px;
                border-radius: 5px;
                position: fixed;
                top: 50%;
                margin-top: -100px;
                left: 50%;
                margin-left: -90px;
                font-size: 20px;
                display: none;
            }

            /*已购买*/
            .glyphicon-ok {
                color: #ff8647;
            }

        </style>

