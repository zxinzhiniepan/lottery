*{
    padding: 0;
    margin: 0;
}
h1,h2,h3,h4,h5,h6,ul,li,div,a,p,div,label,span{
    font-family: 微软雅黑;
    color:#484848;
}
ul,li{
    list-style: none;
}
a{
    text-decoration:none;
}
a:link{
    text-decoration:none;
}
a:visited{
    text-decoration:none;
}
a:hover{
    text-decoration:none;
}



















