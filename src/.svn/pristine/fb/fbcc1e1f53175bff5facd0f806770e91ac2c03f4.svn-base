        <style>
            header {
                position: relative;
                height: 50px;
                background: #ca454c;
            }

            .headtop {
                margin: 0;
                padding: 0;
            }

            .headtop > a {
                position: absolute;
                display: inline-block;
                top: 10px;
                left: 15px;
                width: 28px;
                height: 28px;
                font-size: 30px;
                font-weight: bold;
                color: #fff;
            }

            .headtop > h4 {
                text-align: center;
                padding: 14px 0;
                font-size: 21px;
                color: #fff;
            }
            /*六合彩开奖、分分彩开奖*/
            .row{
                text-align: center;
                height: 55px;
                /*border-bottom: 1px dashed black;*/
            }
            /*标题包裹div*/
            .title-wrap{
                background-color: white;
                border-bottom: 1px solid #d7d7d7;
                padding:5px 0px;

            }

            .title-wrap:first-of-type{
                border-right: 1px solid #d7d7d7;
                border-bottom: 3px solid red;
            }
            /*分分彩开奖*/
            .result-two{
                display: none;
            }

            footer {
                height: 80px;
            }

            .font_but {
                position: fixed;
                bottom: 0;
                width: 100%;
                height: 60px;
                background: #FAFAFA;
            }

            .font_but > ul {
                overflow: hidden;
            }

            .font_but > ul > li {
                float: left;
                width: 25%;
                text-align: center;
                padding: 10px 0;
            }

            .font_but > ul > li > a > i {
                font-size: 22px;
                color: #7A7A7A;
                opacity: 0.74;
            }

            .font_but > ul > li > a {
                display: inline-block;
                width: 100%;
            }

            .font_but > ul > li:nth-child(2) i, .font_but > ul > li:nth-child(2) a p {
                color: #f96237;
            }

            .note_rt > h5 {
                padding: 10px 0;
                text-align: center;
                font-weight: bold;
                border-bottom: 1px solid #eee;
            }

            .note_rt {
                background: #fff;
            }

            .note_rt > h5 > font {
                color: #ee3030;
            }

            .note_rt > ul {
                overflow: hidden;
                width: 70%;
                margin: 0 auto;
            }

            .note_rt > ul > li {
                float: left;
                width: 20%;
                text-align: center;
                padding: 8px;
            }

            .note_rt > ul > li > b {
                display: inline-block;
                width: 30px;
                height: 30px;
                background: #dd3f3f;
                border-radius: 50%;
                color: #fff;
                line-height: 30px;
            }

            .tab_kj {
                overflow: hidden;
            }

            .tab_kj > b {
                border: 1px solid #ccc;
                float: left;
                text-align: center;
                padding: 5px 0;
            }

            .tab_kj > b:first-child {
                width: 38%;
                border-right: 0;

            }

            .tab_kj > b:last-child {
                width: 62%;
            }

            .tab_hm > li > b {
                display: inline-block;
                text-align: center;
            }

            .tab_hm {
                background: #fff;
            }

            .tab_hm > li {
                border-bottom: 1px solid #eee;
                padding: 10px;
            }

            .tab_hm > li > b:first-child {
                width: 37%;
                color: #ee3030;
            }

            .tab_hm > li > b:last-child {
                width: 60%;
                letter-spacing: 10px;
            }
        </style>
    </head>
